{
	"compilerOptions": {
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"noImplicitAny": false,
		"noImplicitReturns": false,	
		"experimentalDecorators": true,	
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"lib" : [ "es2016" ],
		"outDir": "out"
	},
	"exclude": [
		"node_modules"
	]
}